""""""""""""""""""""""""""""""""""""""""""""""""
" My custom and small VIMRC
" Written by MrMikaelDev
""""""""""""""""""""""""""""""""""""""""""""""""

set nocompatible
let g:skip_defaults_vim = 1

syntax on
color desert
mouse-=a

set backspace=indent,eol,start
set autoindent
set nostartofline
set ruler

set noexpandtab
set shiftwidth=4
set tabstop=4

set pastetoggle=<F3>